<Effects>

    <Effect name="explosion">
        
        <Animation id= "explosion" loop = "clamp">
        
            <spriteSheet name="explosion"/>

            <frame coords = "0,0" time = ".1" />
            <frame coords = "1,0" time = ".1" />
            <frame coords = "2,0" time = ".1" />
            <frame coords = "3,0" time = ".1" />
            <frame coords = "4,0" time = ".1" />
            <frame coords = "0,1" time = ".1" />
            <frame coords = "2,1" time = ".1" />
            <frame coords = "3,1" time = ".1" />
            <frame coords = "4,1" time = ".1" />

        </Animation>

    </Effect>


</Effects>